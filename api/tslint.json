{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"quotemark": [
			true,
			"single"
		],
		"indent": [
			true,
			"tabs",
			4
		],
		"eofline": false,
		"trailing-comma": false,
		"only-arrow-functions": false,
		"space-before-function-paren": false,
		"no-console": false,
		"object-literal-sort-keys": false,
		"arrow-parens": [
			true,
			"ban-single-arg-parens"
		],
		"ordered-imports": false,
		"member-access": false
	},
	"rulesDirectory": []
}